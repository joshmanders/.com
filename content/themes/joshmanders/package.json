{
  "name": "joshmanders",
  "description": "The Tailwind CSS based theme for joshmanders.com",
  "version": "13.0.0",
  "author": {
    "name": "Josh Manders",
    "email": "josh@joshmanders.com",
    "url": "https://twitter.com/joshmanders"
  },
  "engines": {
    "ghost": ">=4.0.0",
